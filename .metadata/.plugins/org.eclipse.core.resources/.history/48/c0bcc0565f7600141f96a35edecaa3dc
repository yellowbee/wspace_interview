package com.common.graph;

public class Vertex implements Comparable<Vertex> {
	public String name;
	public int value;
	public Vertex prev;
	public Vertex(String name, int value) {
		this.name = name;
		this.value = value;
	}
	
	@Override
	public int compareTo(Vertex v) {
		return Integer.compare(this.value, v.value);
	}
}
